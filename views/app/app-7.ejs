<div class="up-img-wrap up-half-wrap-tohide" style="margin-bottom: 0px; background: rgba(0,0,0,0) url(img/explorer<%=pictureId%>.jpeg); background-size: cover;">
  <div class="row" style="margin:0px;">
    <% include components/dds_w %>
    <div class="col-xs-6">
    </div>
    <div class="col-xs-3" style="padding:3%; text-align: right;">
      <!-- <img src="img/spotify_w.png" height="100%" width="100%"> -->
      <div class="pic-counter">0</div>
    </div>
  </div><!-- row -->
</div> <!-- Wrapp -->

<div class="bot-img-wrap" style="background-color: grey; text-align: center;">
  <div>
    <div class="sliders-heading"><span>Evaluate this picture adjusting the value of the four sliders</span></div>
    <div style="padding-left: 10%; padding-right: 10%">
      <table class="table borderless">
        <tbody>
          <tr id="explorerImage<%=pictureId%>">
            <td style="width:20%" ><div class="sliders" id="flat-slider<%=pictureId%>1" picID="<%=pictureId%>" sliderID="1" name="Amazing"></div></td>
            <td style="width:20%" ><div class="sliders" id="flat-slider<%=pictureId%>2" picID="<%=pictureId%>" sliderID="2" name="Intense"></div></td>
            <td style="width:20%" ><div class="sliders" id="flat-slider<%=pictureId%>3" picID="<%=pictureId%>" sliderID="3" name="Chills"></div></td>
            <td style="width:20%" ><div class="sliders" id="flat-slider<%=pictureId%>4" picID="<%=pictureId%>" sliderID="4" name="Boring"></div></td>
          </tr>
          <tr style="font-size: 3vw; font-weight: bold; color:white">
            <td><div class="sliders-text">AMAZING</div></td>
            <td><div class="sliders-text">INTENSE</div></td>
            <td><div class="sliders-text">CHILLS</div></td>
            <td><div class="sliders-text">BORING</div></td>
          </tr>
        </tbody>
      </table>
    </div>
  </div>
</div>

<% isHideable='' %>
<% include components/footer %>
